<Window x:Class="EPC232.game.elements.ListImportDialogueBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EPC232.game.elements"
        mc:Ignorable="d"
        Title="List import" Height="170" Width="250" Background="LightGray" WindowStyle="ThreeDBorderWindow"
        WindowStartupLocation="CenterScreen" WindowState="Normal" ResizeMode="NoResize">
    <Grid>
        <Label Content="Which list would you like to import?" Margin="22,22,22,0" Height="26" VerticalAlignment="Top"/>
        <Button x:Name="btnHome" Content="Home" VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="25" Width="80"
            Margin="22,0,142,60" Click="BtnImportHome_Click"/>
        <Button x:Name="btnGuests" Content="Guests" VerticalAlignment="Bottom" HorizontalAlignment="Right" Height="25" Width="80"
            Margin="0,0,22,60" Click="BtnImportGuests_Click"/>
        <Button x:Name="btnClose" Content="Close" VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="25" Width="80"
            Margin="82,0,82,10" Click="hideWindow"/>

    </Grid>
</Window>
